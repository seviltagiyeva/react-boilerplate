{
  "extends": "airbnb" ,
  "env": {
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true
  },
  "parser": "babel-eslint",
  "plugins": ["react"]
}